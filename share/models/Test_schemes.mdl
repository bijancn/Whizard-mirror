########################################################################
# Test model for Scheme-dependent parameter configuration
model "Test_schemes"
schemes = "std", "alt"

# Independent parameters
select scheme
scheme "std"
  ### DO NOT CHANGE THE ORDER OF THESE PARAMETERS
  parameter  a = 1
  derived    b = 2 * a
scheme "alt"
  ### DO NOT CHANGE THE ORDER OF THESE PARAMETERS
  parameter  b = 3
  derived    a = b / 3
end select
