{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "env": {
    "mocha": true,
    "node": true,
    "browser": true,
    "shared-node-browser": true,
    "jquery": true
  },
  "rules": {
    "object-curly-spacing": [2, "never"],
    "no-console": ["error", { "allow": ["warn", "error"]}],
    "prefer-template": 0
  }
}
