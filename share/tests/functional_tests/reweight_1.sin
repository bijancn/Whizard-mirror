# SINDARIN input for WHIZARD self-test
# Process e- e+ -> mu- mu+

model = "QED"
ee = 0.30286
me = 0
mmu = 0

?logging = true
?openmp_logging = false
?vis_history = false
?integration_timer = false

seed = 0

$method = "omega"
$phs_method = "wood"
$integration_method = "vamp"

process reweight_1_p1 = "e-", "e+" => "mu-", "mu+"

sqrts = 1000

n_events = 1
?unweighted = false
?read_raw = false
?write_raw = false

sample_format = weight_stream

simulate (reweight_1_p1) {
  iterations = 1:1000
  alt_setup = { ee = 2 * ee }
}

