# SINDARIN input for WHIZARD self-test
# Process g g -> t tbar

?logging = true
?openmp_logging = false
?vis_history = false
?integration_timer = false

!!! Tests should be run single-threaded 
openmp_num_threads = 1

model = QCD (default)
alphas = 0.1178 

$method = "omega"
process qcdtest_6_p1 = "g", "g" => "t", "tbar"
process qcdtest_6_p2 = "g", "g" => "u", "ubar" { model = QCD (massive) }

compile ()

iterations = 1:1000
sqrts = 1000

seed = 0
model = QCD (default)
integrate (qcdtest_6_p1)

seed = 0
model = QCD (massive)
mu = mtop
integrate (qcdtest_6_p2)
