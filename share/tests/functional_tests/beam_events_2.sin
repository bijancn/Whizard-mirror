# $Id: beam_events_2.sin 4930 2013-12-05 06:14:27Z jr_reuter $
# SINDARIN input for WHIZARD self-test

model = "SM"

?logging = true
?openmp_logging = false
?vis_history = false
?integration_timer = false
?pacify = true

$method = "omega"
process beam_events_2_p1 = "e-", "e+" => "mu-", "mu+"
process beam_events_2_p2 = "e-", "e+" => "mu-", "mu+"

seed = 0

compile ()

$phs_method = "wood"
$integration_method = "vamp"

sqrts = 500

!!! Tests should be run single-threaded 
openmp_num_threads = 1

int n = 1000

cuts = sqrts_hat > 50

beams = "e-", "e+" => isr
integrate (beam_events_2_p1) { iterations = 5:n:"wg", 3:n }

beams = "e-", "e+" => beam_events => isr
$beam_events_file = "uniform_spread_2.5%.dat"
integrate (beam_events_2_p2) { iterations = 5:n:"wg", 3:n }
