# SINDARIN file for WHIZARD self test
# Input file for testing user defined amplitudes

?logging = true
?openmp_logging = false
?vis_history = false
?integration_timer = false

$method = "omega"
$born_me_method = "dummy"
process user_prc_p1 = e1, E1 => e2, E2

!!! Tests should be run single-threaded 
openmp_num_threads = 1

me = 0
sqrts = 175
iterations = 1:10000

seed = 0
integrate (user_prc_p1)

model = SM_tt_threshold
$born_me_method = "threshold"
sqrtsmin = 175 GeV
sqrtsmax = 175 GeV
sqrtsstepsize = 0.2 GeV
m1S = 172 GeV
process user_prc_p2 = e1, E1 => Wp, Wm, b, B
integrate (user_prc_p2)
