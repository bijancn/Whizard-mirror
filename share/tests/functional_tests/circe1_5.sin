# SINDARIN input for WHIZARD self-test

model = "SM"

?logging = true
?openmp_logging = false
?vis_history = false
?integration_timer = false
?pacify = true

$method = "omega"
process circe1_5_p1 = "e-", "e+" => "mu-", "mu+"
process circe1_5_p2 = "e-", "e+" => "mu-", "mu+"
process circe1_5_p3 = "e-", "e+" => "mu-", "mu+"

compile ()

$phs_method = "wood"
$integration_method = "vamp"

sqrts = 500

!!! Tests should be run single-threaded 
openmp_num_threads = 1

int n = 1000
cuts = sqrts_hat > 50
    
seed = 0
integrate (circe1_5_p1) { iterations = 3:n:"g", 3:n }

beams = "e-", "e+" => isr
seed = 0
integrate (circe1_5_p2) { iterations = 8:n:"g", 3:n }

beams = "e-", "e+" => circe1 => isr
$circe1_acc = "TESLA"
circe1_chat = 2
circe1_eps = 1e-5
?circe1_generate = false

seed = 0
integrate (circe1_5_p3) { iterations = 8:n:"wg", 3:n }
