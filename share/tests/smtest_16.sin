# SINDARIN input for WHIZARD self-test
# Testing NLO/LO ratio

model = "SM"

?logging = true
?openmp_logging = false
?vis_history = false
?integration_timer = false
?pacify = true

# This flag is needed to avoid the program searching for loop libraries
?use_gosam_loops = false

process smtest_16_p1 = e1, E1 => u, U { nlo_calculation = "Real" }

seed = 0

iterations = 2:9000
sqrts = 250

integrate (smtest_16_p1)
