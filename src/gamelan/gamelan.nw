Glocs_.bot = "bot";
Glocs_.top = "top";
Glocs_.llft = "lft, bot";
Glocs_.ulft = "lft, top";
Glocs_.lrt  = "rt, bot";
Glocs_.urt  = "rt, top";
Glocs_.lr = "lft, rt" ;
Glocs_.bt = "bot, top";
@ %def Glocs_
